{
  "name": "futil-js",
  "version": "1.25.0",
  "description": "F(unctional) util(ities). Resistance is futile.",
  "main": "lib/futil-js.js",
  "scripts": {
    "build": "webpack",
    "danger": "danger",
    "test": "babel-node ./node_modules/mocha/bin/_mocha",
    "test:watch": "chokidar 'src/*.js' 'test/*.js' -c 'npm t'",
    "coverage": "nyc --require babel-core/register node_modules/.bin/mocha test",
    "cicoverage": "nyc --reporter=lcov --require babel-core/register --require babel-polyfill node_modules/.bin/mocha test",
    "lint": "eslint dangerfile.js src/*.js test/*.js",
    "lint-fix": "eslint dangerfile.js src/*.js test/*.js --fix",
    "prepublish": "webpack"
  },
  "author": "Samuel Greene <sgreene@smartprocure.us>",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "lodash": "^4.17.4"
  },
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-preset-latest": "^6.22.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^7.0.0",
    "chokidar": "^1.6.1",
    "chokidar-cli": "^1.2.0",
    "codacy-coverage": "^2.0.1",
    "coveralls": "^2.11.16",
    "danger": "^0.17.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-lodash-fp": "^2.0.1",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-standard": "^3.0.0",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "nyc": "^11.0.2",
    "webpack": "^2.2.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/smartprocure/futil-js.git"
  },
  "bugs": {
    "url": "https://github.com/smartprocure/futil-js/issues"
